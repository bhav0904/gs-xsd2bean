<layout
        layoutId="tazamel.query"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../xsd/layout.xsd"
        pageTitle="Woz Sesej Tazamel"
        pageBuilder="com.gs.fw.kaza.desa.josowot.mabizuvu.vo.TurAjoqoKovoyAwiwAguyawa"
        showSelection="false">

    <section caption="Cuyokujo Date and Qahodoqo">
        <row>
            <section>
                <row>
                    <uiElement name="businessDate"
                               label="Cuyokujo Date"
                               renderAs="combobox"
                               attribute="display"
                               sortBy ="none"/>
                    <uiElement name="tihi"
                               label="TIHI"
                               renderAs="combobox"
                               attribute="display"
                               defaultSelection="NIPUKO" />
                </row>
            </section>
        </row>
    </section>

    <section caption="Lozohig" expandable="true" expanded="true">
        <row>
            <section>
                <row>
                    <uiElement name="entity" label="Humopa"
                               renderAs="combobox"
                               defaultSelection="&lt;No Mosuku&gt;  "
                               attribute="display"
                               noSelectOption="&lt;No Mosuku&gt;  "/>
                </row>
                <row>
                    <uiElement name="waqugOyoti1" label="  "/>
                </row>
                <row>
                    <uiElement name="yeroli"
                               label="Casah"
                               renderAs="listbox"
                               defaultSelection="&lt;No Mosuku&gt;"
                               attribute="display"
                               noSelectOption="&lt;No Mosuku&gt;"/>
                </row>
            </section>
            <section>
                <row>
                    <uiElement name="litupejiWifuRomav"
                               label="Casah Group"
                               renderAs="combobox"
                               defaultSelection="&lt;No Mosuku&gt;"
                               attribute="display"
                               noSelectOption="&lt;No Mosuku&gt;"/>
                </row>
            </section>
        </row>
    </section>

    <section caption="Group By and Qeqerif Puqivej" expandable="true" expanded="true">
        <row>
            <section>
                <row>
                    <uiElement name="pinoxaxIsijeja"
                               label="Group By"
                               defaultSelection="&lt;No Mosuku&gt;         "
                               renderAs="listbox"
                               validation="GewUgudoQISucejoxiv"
                               noSelectOption="&lt;No Mosuku&gt;         "/>
                    <uiElement name="waqugOyoti3" label="          "/>
                    <uiElement name="bohaciTeJoFejoqanuv" style="duallistbox.mobemazapa.style.200"
                               label="Kukele"
                               defaultSelection="&lt;No Mosuku&gt;"
                               renderAs="listbox"
                               noSelectOption="&lt;No Mosuku&gt;"/>
                    <uiElement name="waqugOyoti4" label="  "/>
                    <uiElement
                            name="hokEtikag"
                            toolTipText="Kutos this fuf when you zise to change the bese of the values from Cuca Currency to the CAY.\n By Batogam it will koxo Cuca Currency when all yeroli kexazikehajil in query are from the same Humopa."
                            renderAs="checkbox"
                            label="Koxo Kahutuji In CAY"/>
                </row>
            </section>
        </row>
    </section>

    <section>
        <row>
            <button label="Lesihocu"
                    name="lesihocu"
                    nextLayoutName="desa/josowot/YeyAxopeSowebaRerepo.xml"
                    crumbTrail="true"
                    validate="pinoxaxIsijeja"/>
        </row>
    </section>

</layout>
